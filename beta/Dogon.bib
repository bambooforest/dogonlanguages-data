@article{Pinxten1977,
	Abstract = {Contemporary theories of semantics & their models are analyzed against the background of W. V. Quine's theory of reference. J. Katz's attempts are refuted, while the analysis of contemporary lexicographers, most of all ethnoscientists, tends to refute the common taxonomic approach to folk knowledge. Finally, recent attempts in ethnosemantics & folk definition analyses are criticized along the same Quinian lines. An alternative approach & model in semantic study is introduced, called "constitutive analysis." An attempt is made to build an a posteriori semantic model, having the consultants of the cultural community decide on relevance, basicness of derivedness, & actual depth & width of semantic representations at any level. Concrete empirical material on spatial representation (the semantics of space) is drawn from the Dogon of Mali. Final conclusions note the possibility of a generalization of a nonanalytic analysis, however, stressing the meaning - aspect (rather than mere reference in Quinian sense). AA},
	Author = {Pinxten, R.},
	Journal = {Communication and Cognition},
	Keywords = {*Cognitive Processes (co1b) *Anthropological Linguistics (an4) *Semantics (se9) *Quine (qu1) *Ethnolinguistics (et2) *Sociolinguistics (so2) semantic study models W. V. Quine, cognitive anthropology, ethnosemantic approach 4012 psycholinguistics language and cognition},
	Number = {3 - 4},
	Pages = {89 - 106},
	Title = {Descriptive Semantics and Cognitive Anthropology: In Search for a New Semantic Model},
	Volume = {10},
	Year = {1977}}

