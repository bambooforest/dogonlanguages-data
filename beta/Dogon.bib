@incollection{Culy1990,
	Abstract = {In internally headed relative clauses (IHRCs), the noun being modified is not extracted but remains in situ. It is shown that in some languages with IHRCs, the form of the highest verb in a relative clause depends on the grammatical function of the head of the relative clause. A means of capturing this generalization in a head - driven phrase structure grammar (HPSG) is described & the framework is shown to predict the importance of the subject /nonsubject distinction in these clauses. Data from Quechua, Fula, Dogon, & other languages are presented to support the claims. 15 References. B. Annesser Murray},
	Address = {Stanford},
