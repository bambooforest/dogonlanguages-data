@article{Olson2004,
	Abstract = {This paper examines the presently received classification of the Niger - Congo language family, with the aim of evaluating the classification & suggesting areas of further research. We find that the exact placement of Ijoid, Kru, & Dogon within the Niger - Congo tree remains to be determined & that the unity of each of the following nodes needs to be better established: Atlantic, (new) Kwa, (new) Benue - Congo, Wide Bantu, Narrow Bantu, & Adamawa - Ubangi. The macro classification is based on the method of resemblances, with refining based on lexicostatistics paired with the examination of shared innovations. The comparative method has mostly been applied at a micro level to specific subgroups. A comprehensive reconstruction of Niger - Congo, including the establishment of sound laws, remains the major future task. 1 Figure, 56 References. Adapted from the source document},
	Author = {Olson, Kenneth},
	Journal = {SIL Electronic Working Papers},
